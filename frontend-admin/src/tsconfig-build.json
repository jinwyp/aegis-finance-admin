{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "sourceMap": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "removeComments": false,
    "noImplicitAny": false,
    "suppressImplicitAnyIndexErrors": true,
    "outDir": "./jsoutput-temp-prodution",
    "rootDir": "./tssource-temp-prodution"
  },
  "exclude": [
    ".git",
    ".js",
    "js",
    "node_modules"
  ]
}