  <process id="" name="process1" isExecutable="true">
    <userTask id="assignOnlineTrader" name="分配线上交易员">
      <documentation>分配线上交易员的管理员分配显示交易员。</documentation>
    </userTask>
    <userTask id="onlineTraderAddMaterial" name="线上交易员填写材料">
      <documentation>线上交易员填写材料</documentation>
    </userTask>
    <userTask id="assignSalesman" name="分配业务员"></userTask>
    <sequenceFlow id="flow4" sourceRef="onlineTraderAddMaterial" targetRef="assignSalesman"></sequenceFlow>
    <userTask id="salesmanAudit" name="业务员审核并填写材料"></userTask>
    <sequenceFlow id="flow5" sourceRef="assignSalesman" targetRef="salesmanAudit"></sequenceFlow>
    <endEvent id="EndBySalesman" name="业务员审核不通过，流程结束"></endEvent>
    <userTask id="assignInvestigator" name="分配尽调员" activiti:assignee="${jty}"></userTask>
    <sequenceFlow id="flow6" sourceRef="salesmanAudit" targetRef="exclusivegateway1"></sequenceFlow>
    <userTask id="investigatorAudit" name="尽调员审核"></userTask>
    <sequenceFlow id="flow10" sourceRef="assignInvestigator" targetRef="investigatorAudit"></sequenceFlow>
    <userTask id="salesmanSupplyInvestigationMaterial" name="业务员补充尽调材料"></userTask>
    <endEvent id="completeFinanceWorkFlow" name="申请成功，流程完成。"></endEvent>
    <exclusiveGateway id="exclusivegateway2" name="Exclusive Gateway"></exclusiveGateway>
    <sequenceFlow id="flow11" sourceRef="investigatorAudit" targetRef="exclusivegateway2"></sequenceFlow>
    <sequenceFlow id="flow12" sourceRef="exclusivegateway2" targetRef="salesmanSupplyInvestigationMaterial">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${needSalesmanInvestigationMaterial == true}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow13" sourceRef="salesmanSupplyInvestigationMaterial" targetRef="investigatorAudit"></sequenceFlow>
    <exclusiveGateway id="exclusivegateway3" name="Exclusive Gateway"></exclusiveGateway>
    <sequenceFlow id="flow14" sourceRef="exclusivegateway2" targetRef="exclusivegateway3">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${needSalesmanMaterial == false}]]></conditionExpression>
    </sequenceFlow>
    <endEvent id="EndByAdjustMan" name="尽调员审核不通过，流程结束"></endEvent>
    <sequenceFlow id="flow15" sourceRef="exclusivegateway3" targetRef="EndByAdjustMan">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${investigatorAudit == false}]]></conditionExpression>
    </sequenceFlow>
    <userTask id="assignRiskManager" name="分配风控人员"></userTask>
    <sequenceFlow id="flow16" sourceRef="exclusivegateway3" targetRef="assignRiskManager">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${investigatorAudit == true && financeType == 1}]]></conditionExpression>
    </sequenceFlow>
    <userTask id="riskManagerAudit" name="风控人员审核"></userTask>
    <sequenceFlow id="flow17" sourceRef="assignRiskManager" targetRef="riskManagerAudit"></sequenceFlow>
    <exclusiveGateway id="exclusivegateway4" name="Exclusive Gateway"></exclusiveGateway>
    <sequenceFlow id="flow18" sourceRef="riskManagerAudit" targetRef="exclusivegateway4"></sequenceFlow>
    <userTask id="needInvestigatorMaterial" name="尽调员补充材料"></userTask>
    <sequenceFlow id="flow19" sourceRef="exclusivegateway4" targetRef="needInvestigatorMaterial">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${needInvestigatorMaterial == true}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow20" sourceRef="needInvestigatorMaterial" targetRef="riskManagerAudit"></sequenceFlow>
    <sequenceFlow id="flow21" sourceRef="exclusivegateway4" targetRef="exclusivegateway5">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${needInvestigatorMaterial == false}]]></conditionExpression>
    </sequenceFlow>
    <endEvent id="EndByRiskManager" name="风控审核不通过，流程结束"></endEvent>
    <userTask id="riskManagerAuditSuccess" name="完成上传合同模板，通知用户"></userTask>
    <sequenceFlow id="flow24" sourceRef="riskManagerAuditSuccess" targetRef="completeFinanceWorkFlow"></sequenceFlow>
    <userTask id="supervisorAudit" name="监管员审核"></userTask>
    <exclusiveGateway id="exclusivegateway6" name="Exclusive Gateway"></exclusiveGateway>
    <sequenceFlow id="flow28" sourceRef="supervisorAudit" targetRef="exclusivegateway6"></sequenceFlow>
    <userTask id="salesmanSupplySupervisionMaterial" name="业务员补充监管材料"></userTask>
    <sequenceFlow id="flow29" sourceRef="exclusivegateway6" targetRef="salesmanSupplySupervisionMaterial">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${needSalesmanSupervisionMaterial == true}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow30" sourceRef="salesmanSupplySupervisionMaterial" targetRef="supervisorAudit"></sequenceFlow>
    <exclusiveGateway id="exclusivegateway7" name="Exclusive Gateway"></exclusiveGateway>
    <sequenceFlow id="flow31" sourceRef="exclusivegateway6" targetRef="exclusivegateway7">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${needSalesmanSupervisionMaterial == false}]]></conditionExpression>
    </sequenceFlow>
    <endEvent id="EndBySupervisor" name="监管员审核不通过，流程结束。"></endEvent>
    <sequenceFlow id="flow32" sourceRef="exclusivegateway7" targetRef="EndBySupervisor">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${supervisorAudit == false}]]></conditionExpression>
    </sequenceFlow>
    <parallelGateway id="parallelgateway2" name="Parallel Gateway"></parallelGateway>
    <sequenceFlow id="flow34" sourceRef="exclusivegateway7" targetRef="parallelgateway2">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${supervisorAudit == true}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow35" sourceRef="exclusivegateway3" targetRef="parallelgateway2">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${investigatorAudit == true && (financeType == 2 || financeType == 3)}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow36" sourceRef="parallelgateway2" targetRef="assignRiskManager">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${investigatorAudit == true &&  && financeType == 1}]]></conditionExpression>
    </sequenceFlow>
    <startEvent id="startFinanceWorkFlow" name="客户申请金融服务">
      <documentation>客户申请金融服务</documentation>
    </startEvent>
    <sequenceFlow id="flow37" sourceRef="startFinanceWorkFlow" targetRef="assignOnlineTrader"></sequenceFlow>
  </process>
